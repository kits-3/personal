package OOP;

import java.util.List;

public abstract class Machine<T> {

	private Integer id;
	private String name;
	protected List<T> listProducts;
	
	

	public Machine(Integer id, String name, List<T> listProducts) {
		super();
		this.id = id;
		this.name = name;
		this.listProducts = listProducts;
	}

	public Machine() {

	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}



}
