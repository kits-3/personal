package JDBC;

import Dao.UserDao;
import model.Dto;

public class Main {
	public static void main(String[] args) {
		Dto dto = new Dto();
		dto.setUserName("Thanh Thinh");
		dto.setPassWord("123456");
		dto.setRoleId(1);
		
		
		UserDao dao = new UserDao();
		dao.add(dto);
		
		dao.findAll();
	}

}
