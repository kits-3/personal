
public class printStar {

	public static void printStar1(int n) {
		
		for(int i=1;i<=n;i++) {
			
			for(int j=n;j>=i;j--) {
				
				System.out.print(" ");
			}
			
			for(int k=0;k<2*i-1;k++) {
				System.out.println("*");
			}
		}
		
		
	}
	
	
	
	public static void main(String[] args) {
		printStar1(n);
	}
	
	
	
	
	
}
