package Vending_Machine;

public class Product {

	private String name_Product;
	private double price;

	Product() {
	}

	Product(String name, double price) {
		this.name_Product = name;
		this.price = price;
	}

	public String getName() {
		return name_Product;
	}
	
	public double getPrice() {
		return price;
	}
	
	

}
