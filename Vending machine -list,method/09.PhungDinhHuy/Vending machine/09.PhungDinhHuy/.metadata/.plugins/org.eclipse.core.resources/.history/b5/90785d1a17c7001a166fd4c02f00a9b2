import java.util.Scanner;

public class While_Example {

	public static void for_ex1(){
		
		Scanner sc=new Scanner(System.in);
		
		int a=sc.nextInt();
		System.out.println("input = "+a);
		
		for(int i=0;i<a;i++) {
			System.out.println(i+"x3 = "+i*3);
			
		}
		
	
	}
	
	
	public static void main(String[] args) {
		for_ex1();
	}
	
	
}
