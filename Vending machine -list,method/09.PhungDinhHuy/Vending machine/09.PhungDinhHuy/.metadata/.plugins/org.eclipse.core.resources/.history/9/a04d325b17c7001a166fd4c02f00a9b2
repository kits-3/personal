import java.util.Scanner;

public class While_Example {

	public static void for_ex1() {

		Scanner sc = new Scanner(System.in);

		String a = sc.next();
		System.out.println("input = " + a);

		if (a.equals("start")) {

			for (int i = 10; i > 0; i--) {
				System.out.println(i + "x3 = " + i * 3);
				

			}

		}
	}

	public static void main(String[] args) {
		for_ex1();
	}

}
